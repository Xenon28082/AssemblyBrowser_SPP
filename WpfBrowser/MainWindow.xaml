<Window x:Class="WpfBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBrowser"
        mc:Ignorable="d"
        Title="AssemblyBrowser" Height="450" Width="800">
    <DockPanel Margin="10">
        <WrapPanel HorizontalAlignment="Center" DockPanel.Dock="Top" Margin="0,0,0,10">
            <Button x:Name="btnOpenFile" Width="100" Height="30" Click="BtnOpenFile_OnClick">
                Open File
            </Button>    
        </WrapPanel>
        <TreeView Margin="10,0,0,13" Name="tree">
        </TreeView>
    </DockPanel>
</Window>
